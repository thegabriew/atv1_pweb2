function contadorvogais(str) {
  str = str.toLowerCase();

  const vogais = "aeiou";

  let contador = 0;

  for (let char of str) {
    if (vogais.includes(char)) {
      contador++;
    }
  }

  return contador;
}

const texto = "Lorem ipsum";
console.log(`A frase possui ${contadorvogais(texto)} vogais.`);
